<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/profile_navigation"
    app:startDestination="@id/profileFragment">

    <fragment
        android:id="@+id/profileFragment"
        android:name="com.hedvig.android.owldroid.ui.profile.ProfileFragment"
        android:label="ProfileFragment">

        <action
            android:id="@+id/action_profileFragment_to_myInfoFragment"
            app:destination="@+id/myInfoFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@+id/profileFragment" />

        <action
            android:id="@+id/action_profileFragment_to_paymentFragment"
            app:destination="@id/paymentFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@+id/profileFragment" />

        <action
            android:id="@+id/action_profileFragment_to_myHomeFragment"
            app:destination="@id/myHomeFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@+id/profileFragment" />

        <action
            android:id="@+id/action_profileFragment_to_feedbackFragment"
            app:destination="@id/feedbackFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@+id/profileFragment" />

        <action
            android:id="@+id/action_profileFragment_to_coinsuredFragment"
            app:destination="@+id/coinsuredFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@+id/profileFragment" />

        <action
            android:id="@+id/action_profileFragment_to_charityFragment"
            app:destination="@id/charityFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@+id/profileFragment" />

        <action
            android:id="@+id/action_profileFragment_to_aboutAppFragment"
            app:destination="@id/aboutAppFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@+id/profileFragment" />

        <action
            android:id="@+id/action_profileFragment_to_referralFragment"
            app:destination="@id/referralFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@id/profileFragment" />
    </fragment>

    <fragment
        android:id="@+id/paymentFragment"
        android:name="com.hedvig.android.owldroid.ui.profile.payment.PaymentFragment"
        android:label="PaymentFragment">

        <action
            android:id="@+id/action_paymentFragment_to_trustlyFragment"
            app:destination="@id/trustlyFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@+id/paymentFragment" />
    </fragment>

    <fragment
        android:id="@+id/myInfoFragment"
        android:name="com.hedvig.android.owldroid.ui.profile.myinfo.MyInfoFragment"
        android:label="MyInfoFragment" />

    <fragment
        android:id="@+id/myHomeFragment"
        android:name="com.hedvig.android.owldroid.ui.profile.myhome.MyHomeFragment"
        android:label="MyHomeFragment" />

    <fragment
        android:id="@+id/feedbackFragment"
        android:name="com.hedvig.android.owldroid.ui.profile.feedback.FeedbackFragment"
        android:label="FeedbackFragment" />

    <fragment
        android:id="@+id/coinsuredFragment"
        android:name="com.hedvig.android.owldroid.ui.profile.coinsured.CoinsuredFragment"
        android:label="CoinsuredFragment" />

    <fragment
        android:id="@+id/charityFragment"
        android:name="com.hedvig.android.owldroid.ui.profile.charity.CharityFragment"
        android:label="CharityFragment" />

    <fragment
        android:id="@+id/aboutAppFragment"
        android:name="com.hedvig.android.owldroid.ui.profile.aboutapp.AboutAppFragment"
        android:label="AboutAppFragment">

        <action
            android:id="@+id/action_aboutAppFragment_to_licensesFragment"
            app:destination="@id/licensesFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            app:popUpTo="@+id/aboutAppFragment" />
    </fragment>

    <fragment
        android:id="@+id/trustlyFragment"
        android:name="com.hedvig.android.owldroid.ui.profile.payment.TrustlyFragment"
        android:label="TrustlyFragment" />

    <fragment
        android:id="@+id/licensesFragment"
        android:name="com.hedvig.android.owldroid.ui.profile.aboutapp.LicensesFragment"
        android:label="LicensesFragment" />

    <fragment
        android:id="@+id/referralFragment"
        android:name="com.hedvig.android.owldroid.ui.profile.referral.ReferralFragment"
        android:label="ReferralFragment" />

</navigation>